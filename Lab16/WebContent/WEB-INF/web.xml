<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Lab16</display-name>
  <context-param>
  <param-name>website</param-name>
  <param-value>www.jlcindia.com</param-value>
  </context-param>
  <servlet>
  <servlet-name>helloServlet</servlet-name>
  <servlet-class>com.jlcindia.servlet.HelloServlet</servlet-class>
  <init-param>
  <param-name>email</param-name>
  <param-value>hellosri@jlc.com</param-value>
  </init-param>
  </servlet>
  <servlet-mapping>
  <servlet-name>helloServlet</servlet-name>
  <url-pattern>/hello.jlc</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>haiServlet</servlet-name>
  <servlet-class>com.jlcindia.servlet.HaiServlet</servlet-class>
  <init-param>
  <param-name>email</param-name>
  <param-value>hainivas@jlc.com</param-value>
  </init-param>
  </servlet>
  <servlet-mapping>
  <servlet-name>haiServlet</servlet-name>
  <url-pattern>/hai.jlc</url-pattern>
  </servlet-mapping>
  
  
  </web-app>